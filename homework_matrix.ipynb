    "import numpy as np\n",
    "\n",
    "mat_a = np.array([[1, 2, 1],\n",
    "                  [0, 1, 1],\n",
    "                  [-1, 1, 1]])\n",
    "mat_b = np.array([[0, -1, 1],\n",
    "                  [1, 1, 2],\n",
    "                  [1, 0, 1]])\n",
    "B = np.linalg.inv(mat_a) @ mat_b\n",
    "print(B)"
   ],
